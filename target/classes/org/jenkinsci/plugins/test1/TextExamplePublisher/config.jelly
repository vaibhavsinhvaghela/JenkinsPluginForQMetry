<?jelly escape-by-default='true'?>
<j:jelly xmlns:j="jelly:core" xmlns:st="jelly:stapler" xmlns:d="jelly:define" xmlns:l="/lib/layout" xmlns:t="/lib/hudson" xmlns:f="/lib/form">
  <!--
    This jelly script is used for per-project configuration.

    See global.jelly for a general discussion about jelly script.
  -->

  <!--
    Creates a text field that shows the value of the "name" property.
    When submitted, it will be passed to the corresponding constructor parameter.
  -->
  <f:entry title="API Key" field="apikey">
    <f:textbox />
  </f:entry>
  <f:entry title="QTM4J URL" field="qtm4jurl">
    <f:textbox />
  </f:entry>
   <f:entry title="File" field="file">
    <f:textbox />
  </f:entry>
   <f:entry title="Test Run" field="testrunname">
    <f:textbox />
  </f:entry>
   <f:entry title="Labels" field="labels">
    <f:textbox />
  </f:entry>
   <f:entry title="Sprint" field="sprint">
    <f:textbox />
  </f:entry>
   <f:entry title="Version" field="version">
    <f:textbox />
  </f:entry>
   <f:entry title="Component" field="component">
    <f:textbox />
  </f:entry>
  <f:entry title="Format" field="selection">
  <f:select/>
</f:entry>
</j:jelly>
